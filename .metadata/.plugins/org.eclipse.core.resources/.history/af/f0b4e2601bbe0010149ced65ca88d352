server:
  port: 8888

spring:
  application:
    name: config-server
  cloud:
    config:
      server:
        git:
          uri: https://github.com/<your-username>/<your-config-repo>.git
          # If repo is private, you can use token-based access:
          # username: <your-username>
          # password: <your-personal-access-token>
          clone-on-start: true
          default-label: main

